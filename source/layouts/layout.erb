    </head>
    <body class="<%= page_classes %>">
        <!--[if lt IE 7]>
            <p class="chromeframe">You are using an outdated browser. <a href="http://browsehappy.com/">Upgrade your browser today</a> or <a href="http://www.google.com/chromeframe/?redirect=true">install Google Chrome Frame</a> to better experience this site.</p>
        <![endif]-->
        
        <header class="cf site-header">
            <div class="wrapper">
                <a href="/" class="site-logo">
                    <%= image_tag 'dot.gif', :class => "logo", :height => "1", :width => "1", :alt => "FloDesign Logo" %>

                </a>
                
                <nav>
                    <a href="/" class="mob-nav-btn"><span aria-hidden="true" data-icon="&#xe623;"></a>
                    <ul class="main-nav">
                        <!-- <li><a href="/work/" class="work_link">Work</a></li> -->
                        <li><%= link_to 'Work', '/work/index.html', :class => "work_link" %></li>
                        <!-- <li><a href="/about/" class="about_link">About</a></li> -->
                        <li><%= link_to 'About', '/about.html', :class => "about_link" %></li>
                        <!-- <li><a href="services.html" class="services_link">Services</a></li> -->
                        <!-- <li><a href="/blog/" class="blog_link">Blog</a></li> -->
                        <li><%= link_to 'Blog', '/blog.html', :class => "blog_link" %></li>
                        <!-- <li><a href="/contact/" class="hire_link">Hire Us</a></li> -->
                        <li><%= link_to 'Hire Us', '/hire-us.html', :class => "hire_link" %></li>
                      
                    </ul>
                </nav>
            </div><!--end wrapper-->
        </header>

        <%= yield %>

        <footer>
            <div class="wrapper">
                <div class="row cf">
                    <div class="col7">
                        <h5>We're social</h5>
                        <ul class="social-links">
                            <li><a href="https://twitter.com/flodesign"><span aria-hidden="true" data-icon="&#xe617;"></span></a></li>
                            <li><a href="https://www.facebook.com/flodesignltd"><span aria-hidden="true" data-icon="&#xe616;"></span></a></li>
                            <li><a href="http://instagram.com/flodesignltd"><span aria-hidden="true" data-icon="&#xe618;"></span></a></li>
                        </ul>

                        <h5>We're here</h5>
                        <p>Flo Design Limited, Tower Works, Globe Road, Leeds, LS11 5QG</p>

                        <p>&copy; Flo Design Limited 2016 <br/>Registered in England & Wales: 7301868</p>
                    </div><!--end col-->
                    
                    <div class="col5 cf">
                        
                        <%= image_tag 'flo-logo-footer.svg', :class => "footer-logo", :alt => "FloDesign footer logo" %>

                        <nav>
                            <ul class="footer-nav cf">
                                <li><%= link_to 'Work', '/work/index.html' %></li>
                                <li><%= link_to 'About', '/about.html' %></li>
                                <!-- <li><a href="services.html">Services</a></li> -->
                              
                                <li><%= link_to 'Blog', '/blog.html' %></li>
                               
                                <li><%= link_to 'Hire Us', '/hire-us.html' %></li>
                            </ul>
                        </nav>
                    </div><!--end col-->
                </div><!--end row-->
            </div><!--end wrapper-->
        </footer>
        
        


        <script src="//ajax.googleapis.com/ajax/libs/jquery/1.8.0/jquery.min.js"></script>
        <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.8.0.min.js"><\/script>')</script>
        <%= javascript_include_tag  "vendor/jquery.sequence-min.js"%>
        <%= javascript_include_tag  "plugins.js"%>
        <%= javascript_include_tag  "main.js"%>

